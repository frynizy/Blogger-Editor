span><span class=3D"pun">.</span><span class=3D"pln">createElement</span><s=
pan class=3D"pun">(</span><span class=3D"str">'img'</span><span class=3D"pu=
n">);</span><span class=3D"pln">
img</span><span class=3D"pun">.</span><span class=3D"pln">src </span><span =
class=3D"pun">=3D</span><span class=3D"pln"> </span><b><span class=3D"pln">=
